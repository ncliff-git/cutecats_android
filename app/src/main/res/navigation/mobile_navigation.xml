<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/breeds_navigation">

    <include app:graph="@navigation/breeds_navigation"/>

    <fragment
        android:id="@+id/navigation_upload"
        android:name="com.github.ncliff.cutecats.ui.view.upload.UploadFragment"
        android:label="@string/upload"
        tools:layout="@layout/fragment_upload" />

    <fragment
        android:id="@+id/navigation_vote"
        android:name="com.github.ncliff.cutecats.ui.view.vote.VoteFragment"
        android:label="@string/vote"
        tools:layout="@layout/fragment_vote" />
    <fragment
        android:id="@+id/navigation_favourites"
        android:name="com.github.ncliff.cutecats.ui.view.favourites.FavouritesFragment"
        android:label="@string/favourites"
        tools:layout="@layout/fragment_favourites" />

</navigation>